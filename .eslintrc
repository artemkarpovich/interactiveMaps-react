{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
     }
   },
  "rules": {
    "linebreak-style": 0,
    "jsx-boolean-value": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0
  },
  "env": {
     "browser": true,
     "node": true
  },
  "plugins": [
     "react"
  ]
}